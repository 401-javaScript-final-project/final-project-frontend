{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest ./tests"
  },
  "jest": {
    "preset": "jest-expo",
    "rootDir": ".",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ]
  },
  "dependencies": {
    "expo": "^35.0.0",
    "expo-barcode-scanner": "^7.0.0",
    "expo-permissions": "^7.0.0",
    "firebase": "^7.2.3",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-camera": "^3.8.0",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-paper": "^3.1.1",
    "react-native-qrcode": "^0.2.7",
    "react-native-qrcode-scanner": "^1.2.3",
    "react-native-qrcode-svg": "^5.3.1",
    "react-native-svg": "^9.13.3",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-material-bottom-tabs": "^2.1.5",
    "react-navigation-stack": "^1.10.3"
  },
  "devDependencies": {
    "babel-preset-expo": "^7.1.0",
    "firebase-mock": "^2.2.10",
    "jest": "^24.9.0",
    "jest-expo": "^35.0.0",
    "react-test-renderer": "^16.11.0"
  },
  "private": true
}
